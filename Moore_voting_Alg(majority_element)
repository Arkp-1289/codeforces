class Solution {
public:
    int majorityElement(vector<int>& v) {
        int el,c=0,n=v.size();
        for (int i=0;i<n;i++){
            if (!c){el=v[i];c++;}
            else if(el==v[i]){c++;}
            else{c--;}
        }
        // int c2=0;
        // for (int i=0;i<n;i++){
        //     if (el==v[i]){c2++;}
        // }
    return el;
    }
};
